{
  "name": "auto-release-notes",
  "version": "0.2.0",
  "private": true,
  "description": "Enforce release notes conventions for PRs and generate a changelog upon release",
  "module": "lib/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "build-release-plugin": "tsc -p tsconfig.release.json",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build src/main.ts --source-map --license licenses.txt",
    "release": "yarn build-release-plugin && release-it --only-version",
    "test": "jest",
    "all": "yarn run build && yarn run format-check && yarn run package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wandb/auto-release-notes"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^5.0.0",
    "@octokit/rest": "^18.7.2",
    "external-editor": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.171",
    "@types/node": "^18.6.3",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.21",
    "prettier": "2.2.1",
    "release-it": "^14.10.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
